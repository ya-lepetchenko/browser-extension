{
    "manifest_version": 3,
    "name": "OVP extension caller 2.0!",
    "description": "Make calls from browser with OVP telephony",
    "version": "1.0",
    "action": {
      "default_popup": "index3.html"
    },
    "permissions": [
      "contextMenus",
      "activeTab", 
      "scripting",
      "storage",
      "tabGroups",
      "tabs",
      "webRequest",
      "webRequestBlocker",
      "webRequestAuthProvider"
    ],
    "content_scripts": [
      {
        "js":["config-reader.js", "script.js"],
        "matches": ["<all_urls>"]
      }
   ],
    "host_permissions": [
      "https://dev-cabinet.onevoiplanet.ua/*",
      "<all_urls>"
    ]
  }